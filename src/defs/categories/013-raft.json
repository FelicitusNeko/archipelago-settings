{
  "category": "Raft",
  "index": 13,
  "settings": [
    {
      "type": "boolean",
      "name": "use_resource_packs",
      "readableName": "Use resource packs",
      "description": "Uses Resource Packs to fill out the item pool from Raft. Resource Packs have basic earlygame items such as planks, plastic, or food.",
      "default": true
    },
    {
      "type": "numeric",
      "name": "minimum_resource_pack_amount",
      "readableName": "Minimum resource pack amount",
      "description": "The minimum amount of resources available in a resource pack.",
      "low": 1,
      "high": 15,
      "randomable": true,
      "default": 1
    },
    {
      "type": "numeric",
      "name": "maximum_resource_pack_amount",
      "readableName": "Maximum resource pack amount",
      "description": "The maximum amount of resources available in a resource pack.",
      "low": 1,
      "high": 15,
      "randomable": true,
      "default": 5
    },
    {
      "type": "string",
      "name": "duplicate_items",
      "readableName": "Duplicate items",
      "description": "Adds duplicates of items to the item pool. These will be selected alongside Resource Packs (if configured). Note that there are not many progression items, and selecting Progression may produce many of the same duplicate item.",
      "values": {
        "disabled": "Disabled",
        "progression": "Progression only",
        "non_progression": "Non-progression only",
        "any": "Any"
      },
      "default": "disabled"
    },
    {
      "type": "string",
      "name": "island_frequency_locations",
      "readableName": "Island frequency locations",
      "description": "Sets where frequencies for story islands are located.",
      "values": {
        "vanilla": "Vanilla",
        "random_on_island": "Random on island",
        "progressive": "Progressive",
        "anywhere": "Anywhere"
      },
      "default": "random_on_island"
    },
    {
      "type": "boolean",
      "name": "progressive_items",
      "readableName": "Progressive items",
      "description": "Makes some items, like the Bow and Arrow, progressive rather than raw unlocks.",
      "default": true
    },
    {
      "type": "boolean",
      "name": "big_island_early_crafting",
      "readableName": "Big island early crafting",
      "description": "Allows recipes that require items from big islands (e.g. leather) to lock earlygame items like the Receiver, Bolt, or Smelter.",
      "default": false
    },
    {
      "type": "boolean",
      "name": "paddleboard_mode",
      "readableName": "Paddleboard mode",
      "description": "Sets later story islands to in logic without an Engine or Steering Wheel. May require lots of paddling. **Not recommended.**",
      "default": true
    }
  ],
