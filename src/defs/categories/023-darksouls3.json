{
  "category": "Dark Souls III",
  "index": 23,
  "settings": [
    {
      "type": "boolean",
      "name": "auto_equip",
      "readableName": "Auto Equip",
      "description": "Automatically equips any received armor or left/right weapons.",
      "default": false
    },
    {
      "type": "boolean",
      "name": "lock_equip",
      "readableName": "Lock Equipement Slots",
      "description": "Lock the equipment slots so you cannot change your armor or your left/right weapons. Works great with the Auto-equip option.",
      "default": false
    },
    {
      "type": "boolean",
      "name": "no_weapon_requirements",
      "readableName": "No Weapon Requirements",
      "description": "Disable the weapon requirements by removing any movement or damage penalties, permitting you to use any weapon early.",
      "default": false
    },
    {
      "type": "boolean",
      "name": "randomize_weapons_level",
      "readableName": "Randomize weapons level",
      "description": "Enable this option to upgrade 33% (based on the probability chance) of the pool of weapons to a random value between +1 and +5/+10.",
      "default": true
    },
    {
      "type": "boolean",
      "name": "late_basin_of_vows",
      "readableName": "Late Basin of Vows",
      "description": "Force the Basin of Vows to be located as a reward of defeating Pontiff Sulyvahn. It permits to ease the progression by preventing having to kill the Dancer of the Boreal Valley as the first boss.",
      "default": true
    }
  ],
